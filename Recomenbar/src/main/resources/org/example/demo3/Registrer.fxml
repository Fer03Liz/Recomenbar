<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="684.0" prefWidth="608.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo3.RegistrerController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="723.0" prefWidth="680.0">
         <children>
            <Label text="RECOMENBAR" />
            <Label text="Welcome Back" />
            <VBox alignment="CENTER" prefHeight="524.0" prefWidth="680.0">
               <children>
                  <Label text="Nombres:" />
                  <TextField fx:id="nombresField" prefHeight="31.0" prefWidth="488.0" />
                  <Label text="Correo:" />
                  <TextField fx:id="correoField" prefHeight="31.0" prefWidth="488.0" />
                  <Label text="Edad" />
                  <TextField fx:id="edadField" prefHeight="31.0" prefWidth="488.0" />
                  <Label text="Contraseña:" />
                  <PasswordField fx:id="contraseñaField" />
                  <Button mnemonicParsing="false" onAction="#onRegitrarButtonClick" text="Registrar" />
               </children>
            </VBox>
            <Button mnemonicParsing="false" onAction="#onVolverButtonClick" text="Ya estas registrado?" />
         </children>
      </VBox>
      <ButtonBar prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#onHomeButtonClick" text="Home" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>

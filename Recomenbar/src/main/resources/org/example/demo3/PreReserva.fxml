<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo3.PreReservaController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="340.0" layoutY="120.0" prefHeight="480.0" prefWidth="600.0">
         <children>
            <Label alignment="TOP_CENTER" graphicTextGap="69.0" prefHeight="124.0" prefWidth="600.0" style="-fx-font-size: 24px; -fx-font-family: 'Belleza'; -fx-text-fill: white;" text="¿Qué deseas reservar?" />
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../LogoFondoNegro.png" />
               </image>
            </ImageView>
            <HBox alignment="CENTER" prefHeight="299.0" prefWidth="575.0">
               <children>
                  <Button contentDisplay="CENTER" mnemonicParsing="false" onAction="#onDiscotecaButtonClick" prefHeight="106.0" prefWidth="189.0" style="-fx-background-color: #87CEEB; -fx-text-fill: black; -fx-font-family: 'Belleza';" text="Discotecas" />
                  <Button contentDisplay="CENTER" layoutX="106.0" layoutY="78.0" mnemonicParsing="false" onAction="#onEventoButtonClick" prefHeight="106.0" prefWidth="189.0" style="-fx-background-color: #87CEEB; -fx-text-fill: black; -fx-font-family: 'Belleza';" text="Eventos" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</AnchorPane>

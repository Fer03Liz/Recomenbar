<VBox alignment="CENTER" prefHeight="1000.0" prefWidth="2000.0" spacing="20.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo3.HomeController">
    <padding>

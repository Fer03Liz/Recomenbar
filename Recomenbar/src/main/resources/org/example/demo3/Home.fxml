            <Button graphicTextGap="10.0" onAction="#onLoginButtonClick" prefHeight="147.0" prefWidth="359.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Login">
            <font>
               <Font size="69.0" />
            </font></Button>
            <Button layoutX="575.0" layoutY="30.0" onAction="#onRegisterButtonClick" prefHeight="147.0" prefWidth="347.0" style="-fx-background-color: black; -fx-text-fill: white;" text="Register">

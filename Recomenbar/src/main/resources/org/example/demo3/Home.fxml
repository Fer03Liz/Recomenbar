    <Label fx:id="welcomeText1" layoutX="671.0" layoutY="471.0" prefHeight="139.0" prefWidth="394.0" style="-fx-font-size: 69px; -fx-font-family: 'Belleza'; -fx-text-fill: white;" text="BIENVENIDO" />
    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
